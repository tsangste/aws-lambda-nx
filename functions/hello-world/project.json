{
  "root": "functions/hello-world",
  "projectType": "application",
  "sourceRoot": "functions/hello-world/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "functions/hello-world",
        "color": true,
        "command": "sls package"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "functions/hello-world",
        "color": true,
        "command": "sls offline start"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "functions/hello-world",
        "color": true,
        "command": "sls deploy --verbose"
      },
      "dependsOn": [
        {
          "target": "deploy",
          "projects": "dependencies"
        }
      ]
    },
    "remove": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "functions/hello-world",
        "color": true,
        "command": "sls remove"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["functions/hello-world/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["coverage/functions/hello-world"],
      "options": {
        "jestConfig": "functions/hello-world/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["functions"]
}
